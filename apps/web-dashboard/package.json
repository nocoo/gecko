{
  "name": "gecko-web",
  "version": "1.1.0",
  "description": "Gecko screen time dashboard",
  "type": "module",
  "scripts": {
    "dev": "vinext dev --port 7028",
    "build": "vinext build",
    "start": "vinext start --port ${PORT:-7028}",
    "dev:e2e": "E2E_SKIP_AUTH=true vinext dev --port 10728",
    "lint": "eslint",
    "test": "bun test src/__tests__/ --timeout 30000",
    "test:coverage": "bun test src/__tests__/ --coverage --timeout 30000",
    "test:e2e": "RUN_E2E=true bun test --env-file=.env.e2e src/__tests__/e2e/ --timeout 60000",
    "prepare": "husky"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.1",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "9",
    "husky": "^9.1.7",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.0.0",
    "@vitejs/plugin-rsc": "^0.5.20"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.49",
    "@ai-sdk/openai": "^3.0.36",
    "ai": "^6.0.104",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.575.0",
    "next-auth": "^5.0.0-beta.30",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-day-picker": "^9.14.0",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "vinext": "^0.0.9"
  }
}
